@model List<InventarioInformatico.BL.Producto>
@{
    ViewBag.Title = "Index";
}

<h2>Productos</h2>

<table class="table table-bordered table-hover">
    <thead>
        <tr>
            <th>
                Codigo
            </th>
            <th>
                Marca
            </th>
            <th>
                Caracteristicas
            </th>
        </tr>
    </thead>

    @foreach (var producto in Model)
    {
    <tr>
        <td>@producto.Id</td>
        <td>@producto.Marca</td>
        <td>@producto.Caracteristicas</td>
    </tr>
    }

</table>